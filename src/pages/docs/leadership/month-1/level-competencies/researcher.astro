---
import { getEntry } from 'astro:content';
import CompetencyLayout from '../../../../../layouts/CompetencyLayout.astro';

const entry = await getEntry('docs', 'leadership/month-1/level-competencies/researcher');
const { Content } = await entry.render();

const breadcrumbs = [
  { text: 'Leadership', href: '/docs/leadership' },
  { text: 'Month 1', href: '/docs/leadership/month-1' },
  { text: 'Level Competencies', href: '/docs/leadership/month-1/level-competencies' }
];

<CompetencyLayout frontmatter={entry.data} breadcrumbs={breadcrumbs}>
  <Content />
</CompetencyLayout>